{
    "compilerOptions": {
        "target": "es2020",
        "module": "es2020",
        "outDir": "./js",
        "rootDir": "./ts",
        "strict": true,
        "esModuleInterop": true,
        "removeComments": true,
        "noEmitOnError": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["ts/**/*.ts"],
}